from machine import Pin, I2C
from ssd1306 import SSD1306_I2C
import time

i2c = I2C(0, scl=Pin(5), sda=Pin(4), freq=3200000)
oled = SSD1306_I2C(128, 64, i2c)

oled.fill(0)
start=time.ticks_us()
oled.ellipse(64,32,20,20,1,0)
etc = time.ticks_diff(time.ticks_us(), start)
oled.show()
et = time.ticks_diff(time.ticks_us(), start)

print(f'Total ET {et} - Computation Time {etc} = I2c Transfer Time {et-etc}')
while True:
    pass
